    {
      "_id": "jek2rDaYSE3Xjrgr",
      "name": "Роющий червь",
      "type": "feat",
      "img": "icons/environment/creatures/bug-worm-teeth-green.webp",
      "effects": [],
      "folder": "l3uPp5S2He9H36lZ",
      "sort": 100000,
      "flags": {
        "srd5e": {
          "page": "monsterAction",
          "source": "MPMM",
          "hash": "burrowing%20worm_spawn%20of%20kyuss_mpmm_mpmm"
        },
        "midi-qol": {
