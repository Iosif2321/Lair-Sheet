    {
      "_id": "mXITloUOt4pDS3m9",
      "name": "Берсерк",
      "type": "feat",
      "img": "icons/creatures/reptiles/lizard-mouth-glowing-red.webp",
      "effects": [],
      "folder": "6BlejHG12kId0wQz",
      "sort": 100000,
      "flags": {
        "betterRolls5e": {
          "quickOther": {
