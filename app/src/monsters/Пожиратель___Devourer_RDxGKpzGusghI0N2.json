    {
      "_id": "BBxTXhBv7JFPJwsH",
      "name": "Заточение Души",
      "type": "feat",
      "img": "icons/magic/control/debuff-chains-orb-movement-blue.webp",
      "effects": [],
      "folder": "l3uPp5S2He9H36lZ",
