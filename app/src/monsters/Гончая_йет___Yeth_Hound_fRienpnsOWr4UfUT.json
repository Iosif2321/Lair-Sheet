    {
      "_id": "k43CZEadadbjHya3",
      "name": "Зловещий лай",
      "type": "feat",
      "img": "icons/creatures/abilities/dragon-breath-purple.webp",
      "effects": [],
      "folder": "l3uPp5S2He9H36lZ",
      "sort": 100000,
      "flags": {
        "core": {
          "sourceId": "Item.e3NuGGRAum8WIvTA"
        },
        "midi-qol": {
