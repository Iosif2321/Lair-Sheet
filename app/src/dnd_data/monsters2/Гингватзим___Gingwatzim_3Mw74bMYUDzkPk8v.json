    {
      "_id": "vcFXGIO2euQv89lc",
      "name": "Смена формы",
      "type": "feat",
      "img": "icons/magic/control/silhouette-hold-change-blue.webp",
      "effects": [],
      "folder": "DwG2ks73eSgozZVw",
      "sort": 100000,
