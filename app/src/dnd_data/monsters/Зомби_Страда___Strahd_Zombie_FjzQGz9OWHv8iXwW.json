  "items": [
    {
      "_id": "Y2mVuTBkWWlPPIPN",
      "name": "Отвратительные конечности",
      "type": "feat",
      "img": "icons/commodities/biological/hand-clawed-tan.webp",
      "effects": [],
      "folder": "2SS5VabhY0NApQH0",
      "sort": 100000,
      "flags": {
        "srd5e": {
          "page": "monsterTrait",
