    {
      "_id": "UTb5SvYTSZYMwrpo",
      "name": "Жало",
      "type": "weapon",
      "img": "icons/creatures/abilities/fang-tooth-venomous.webp",
      "effects": [],
      "folder": "6BlejHG12kId0wQz",
      "sort": 100000,
      "flags": {
