    {
      "_id": "PBxkZQ7iCrr1DjO7",
      "name": "Фейский шаг",
      "type": "feat",
      "img": "icons/magic/movement/trail-streak-zigzag-teal.webp",
      "effects": [],
      "folder": "PGXzQnp3QpelnqvM",
      "sort": 100000,
      "flags": {
        "dnd5e": {
