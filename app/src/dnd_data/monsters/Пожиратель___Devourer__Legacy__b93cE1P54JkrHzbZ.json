    {
      "_id": "pX1F9z67bRdL6aHl",
      "name": "Заточение Души",
      "type": "feat",
      "img": "icons/magic/control/debuff-chains-orb-movement-blue.webp",
      "effects": [],
      "folder": "8Bqo3jrFgRL9o9dC",
      "sort": 100000,
      "flags": {
        "midi-qol": {
