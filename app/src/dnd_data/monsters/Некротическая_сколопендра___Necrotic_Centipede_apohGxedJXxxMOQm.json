    {
      "_id": "l0VAZ8jUsQNjCYWF",
      "name": "Проглатывание",
      "type": "feat",
      "img": "icons/creatures/abilities/mouth-teeth-tongue-purple.webp",
      "effects": [],
      "folder": null,
      "sort": 0,
      "flags": {
        "core": {
          "sourceId": "Item.Sn5ngnlaMD6vydSX"
        },
        "midi-qol": {
          "effectActivation": false
        },
        "midiProperties": {
          "nodam": false,
          "fulldam": false,
          "halfdam": false,
          "autoFailFriendly": false,
          "autoSaveFriendly": false,
          "rollOther": false,
          "critOther": false,
          "offHandWeapon": false,
