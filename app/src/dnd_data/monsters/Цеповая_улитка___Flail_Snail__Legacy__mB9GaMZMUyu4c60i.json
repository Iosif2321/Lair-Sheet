    {
      "_id": "iLZKRgor1SLSMNUU",
      "name": "Сверкающая Раковина",
      "type": "feat",
      "img": "icons/magic/defensive/shield-barrier-deflect-gold.webp",
      "effects": [],
      "folder": null,
      "sort": 0,
      "flags": {
